{
  "name": "@trendmicro/react-datepicker",
  "version": "1.0.0-alpha.4",
  "description": "React DatePicker Component",
  "main": "lib/index.js",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "prepublish": "npm run eslint && npm test && npm run clean && npm run bowersync && npm run build && npm run build-examples",
    "bowersync": "./scripts/bowersync",
    "build": "webpack && npm run cleancss",
    "build-examples": "cd examples; webpack",
    "clean": "rm -f {lib,dist}/*",
    "cleancss": "cleancss -o dist/react-datepicker.min.css dist/react-datepicker.css",
    "demo": "http-server -p 8000 docs/",
    "eslint": "eslint --ext .js --ext .jsx examples src test",
    "test": "tap test/*.js --node-arg=--require --node-arg=babel-register --node-arg=--require --node-arg=babel-polyfill",
    "coveralls": "tap test/*.js --coverage --coverage-report=text-lcov --nyc-arg=--require --nyc-arg=babel-register --nyc-arg=--require --nyc-arg=babel-polyfill | coveralls",
    "dev": "cd examples; webpack-dev-server --hot --inline --host 0.0.0.0 --port 8000 --content-base ../docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trendmicro-frontend/react-datepicker.git"
  },
  "author": "Cheton Wu <cheton_wu@trend.com.tw>",
  "contributors": [
    {
      "name": "Cheton Wu",
      "email": "cheton_wu@trend.com.tw",
      "url": "https://github.com/trendmicro-frontend"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trendmicro-frontend/react-datepicker/issues"
  },
  "homepage": "https://github.com/trendmicro-frontend/react-datepicker",
  "keywords": [
    "react",
    "date",
    "picker",
    "react-datepicker"
  ],
  "peerDependencies": {
    "moment": ">=2.8.0",
    "react": "^0.14.0 || >=15.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.5.8",
    "react-datepicker": "^1.3.0",
    "uncontrollable": "^5.0.0"
  },
  "devDependencies": {
    "@trendmicro/react-anchor": "~0.5.5",
    "@trendmicro/react-buttons": "~1.3.0",
    "@trendmicro/react-dropdown": "~1.2.2",
    "babel-cli": "~6.26.0",
    "babel-core": "~6.26.0",
    "babel-eslint": "~8.2.2",
    "babel-loader": "~7.1.4",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-react": "~6.24.1",
    "babel-preset-stage-0": "~6.24.1",
    "clean-css": "~4.1.11",
    "clean-css-cli": "~4.1.11",
    "coveralls": "~3.0.0",
    "css-loader": "~0.28.0",
    "eslint": "~4.18.2",
    "eslint-config-trendmicro": "~1.3.0",
    "eslint-loader": "~1.7.1",
    "eslint-plugin-import": "~2.9.0",
    "eslint-plugin-jsx-a11y": "~6.0.3",
    "eslint-plugin-react": "~7.7.0",
    "extract-text-webpack-plugin": "~3.0.0",
    "file-loader": "~0.11.1",
    "find-imports": "~0.5.2",
    "html-webpack-plugin": "~2.30.1",
    "http-server": "~0.11.1",
    "moment": "~2.21.0",
    "nib": "~1.1.2",
    "qs": "~6.5.1",
    "react": "^0.14.0 || >=15.0.0",
    "react-dom": "^0.14.0 || >=15.0.0",
    "style-loader": "~0.18.2",
    "stylint": "~1.5.9",
    "stylint-loader": "~1.0.0",
    "stylus-loader": "~3.0.1",
    "tap": "~11.1.1",
    "trendmicro-ui": "~0.5.1",
    "url-loader": "~0.5.8",
    "webpack": "~3.4.1",
    "webpack-dev-server": "~2.6.1",
    "which": "~1.3.0"
  }
}
